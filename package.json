{
  "name": "sqlite-95",
  "description": "SQLite ORM with a web admin",
  "version": "4.0.0",
  "homepage": "https://github.com/ncordin/sqlite-95",
  "repository": {
    "type": "git",
    "url": "git@github.com:ncordin/sqlite-95.git"
  },
  "main": "index.ts",
  "files": [
    "./index.ts",
    "orm/",
    "admin-router/",
    "controller/",
    "admin-webapp/dist/"
  ],
  "license": "MIT",
  "scripts": {
    "start": "bun admin-router/start.js",
    "test": "bun orm/tests/players.test.ts",
    "build:admin": "cd admin-webapp && yarn build",
    "lint": "yarn run lint:ts && yarn run lint:eslint && yarn run lint:prettier",
    "lint:ts": "tsc",
    "lint:prettier": "prettier --check '*/**/*.{js,ts,tsx}'",
    "lint:eslint": "eslint ./orm ./controller ./admin-router",
    "format": "prettier --write '*/**/*.{js,ts,tsx}'",
    "ci": "yarn run lint",
    "prepublish": "yarn run build:admin"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "bun-types": "^1.0.1",
    "cors": "^2.8.5",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "express": "^4.17.14",
    "prettier": "^2.8.1",
    "typescript": "^5.2.2"
  }
}
