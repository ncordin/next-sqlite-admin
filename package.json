{
  "name": "sqlite-95",
  "description": "A set of Node utilities for building APIs.",
  "version": "2.2.0",
  "homepage": "https://github.com/ncordin/sqlite-95",
  "repository": {
    "type": "git",
    "url": "git@github.com:ncordin/sqlite-95.git"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/",
    "webapp/dist/"
  ],
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node src/admin/start.js",
